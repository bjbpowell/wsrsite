<h1>Listing dealers</h1>

<% @dealers.each do |dealer| %>
<div class="dealerinfo">
    <p><%= dealer.name %><br />
    <%= dealer.address %><br />
    <%= dealer.city %>
    <%= dealer.state %><br />
    <%= dealer.postal %><br />
    <%= link_to 'Show', dealer %><br />
    <%= link_to 'Edit', edit_dealer_path(dealer) %><br />
    <%= link_to 'Destroy', dealer, confirm: 'Are you sure?', method: :delete %>
    </p>
</div>
<% end %>

<br />

<%= link_to 'New Dealer', new_dealer_path %>
